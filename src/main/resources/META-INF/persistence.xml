<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="mclpe" transaction-type="JTA">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <jta-data-source>java:/jdbc/mpcle</jta-data-source>
    <class>mx.gob.imss.dpes.prestamoback.entity.Prestamo</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.Plazo</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.PrestamoRecuperacion</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.AmortizacionDatosBase</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.AmortizacionTabla</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.McltDocumento</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.MclcEstadoReporte</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.MclcSubTipoReporte</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.MclcTipoReporte</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.McltReporte</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.McltReporteDocumento</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.MclcCorreo</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.MclcEntidadFinanciera</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.McltSolicitud</class>
    <class>mx.gob.imss.dpes.prestamoback.entity.McltPrestamoInsumoTa</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect"/>
      <property name="hibernate.show_sql" value="false"/>
      <property name="hibernate.format_sql" value="true"/>
      <property name="hibernate.use_sql_comments" value="true"/>
      <property name="hibernate.max_fetch_depth" value="1"/>
      <property name="hibernate.id.new_generator_mappings" value="true"/>
      <property name="hibernate.jdbc.batch_size" value="1"/>
    </properties>
  </persistence-unit>
</persistence>
